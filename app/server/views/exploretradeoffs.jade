doctype html
meta(charset="utf-8")
html
head
	title No Free Lunch
	link(type="text/css", rel="stylesheet",href="/vendor/bootstrap.min.css")
	link(type="text/css", rel="stylesheet",href="/css/application.css")
	link(type="text/css", rel="stylesheet",href="/css/explore-tradeoffs.css")
	link(type="text/css", rel="stylesheet",href="/css/chart.css")
	script(type="text/javascript" src="/vendor/d3.min.js")
	script(type="text/javascript" src="/js/util.js")
	script(type="text/javascript" src="/js/bar-chart.js")
	script(type="text/javascript" src="/js/review-text.js")
	script(type="text/javascript" src="/js/scatterplot.js")
	script(type="text/javascript" src="/js/explore-tradeoffs.js")

body
	header
		div(class="navbar navbar-fixed-top")
			div(class="container")
				div(class="row")
					div(class="navbar-header")
						div(id="logo")
							a(href="/") No Free Lunch
					div(class="collapse navbar-collapse")
						ul(class="nav navbar-nav")
							li
								a(href="pickfeatures?category="+category) Pick Features
							li 
								a(href="#",class="currently-visited") Explore Trade-offs

	div(class="container")
		div(class="width-600")
			div(id="buttons")
				h4 Choose one or more features to explore trade-offs.
				div(id="relationship-instructions") Click to examine. Scroll for more.
				div(id="feature-buttons")
					-for(var i = 0; i < features.length; i++)
						button(class="btn btn-default btn-sm") #{features[i]}
				div(id="relationships")
					h4 The following relationships were found.

			div(class="charts")




